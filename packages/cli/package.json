{
  "name": "@blysspeak/prada",
  "version": "1.0.0",
  "description": "PRADA - Fast admin for your projects. Instant PostgreSQL database admin panel.",
  "type": "module",
  "bin": {
    "prada": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "tsup src/cli.js --watch --format esm",
    "build": "tsup src/cli.js src/index.js --format esm --clean",
    "start": "node dist/cli.js",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@blysspeak/prada-core": "workspace:*",
    "@blysspeak/prada-server": "workspace:*",
    "@prisma/client": "~6.19.0",
    "citty": "^0.1.6",
    "consola": "^3.4.2",
    "express": "^4.21.0",
    "picocolors": "^1.1.1",
    "prisma": "~6.19.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": "blysspeak",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blysspeak/prada.git",
    "directory": "packages/cli"
  },
  "homepage": "https://github.com/blysspeak/prada#readme",
  "bugs": {
    "url": "https://github.com/blysspeak/prada/issues"
  },
  "keywords": [
    "prisma",
    "admin",
    "postgresql",
    "database",
    "cli",
    "crud",
    "admin-panel",
    "db-admin",
    "npx"
  ]
}
