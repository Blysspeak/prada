{
  "name": "@blysspeak/prada-ui",
  "version": "1.0.0",
  "description": "PRADA UI - React Admin Panel Interface",
  "type": "module",
  "main": "./dist/lib/index.js",
  "module": "./dist/lib/index.js",
  "types": "./dist/lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/lib/index.js",
      "types": "./dist/lib/index.d.ts"
    },
    "./dist/*": "./dist/*",
    "./styles.css": "./dist/lib/style.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && vite build --mode library && tsc --emitDeclarationOnly --outDir dist/lib",
    "build:app": "vite build",
    "build:lib": "vite build --mode library && tsc --emitDeclarationOnly --outDir dist/lib",
    "preview": "vite preview",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@fontsource-variable/geist": "^5.2.8",
    "@fontsource-variable/geist-mono": "^5.2.7",
    "@tanstack/react-query": "^5.62.0",
    "@tanstack/react-table": "^8.20.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.460.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.0",
    "react-router-dom": "^7.1.0",
    "tailwind-merge": "^3.4.0",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.7.0",
    "vite": "^6.0.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": "blysspeak",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blysspeak/prada.git",
    "directory": "packages/ui"
  },
  "homepage": "https://github.com/blysspeak/prada#readme",
  "bugs": {
    "url": "https://github.com/blysspeak/prada/issues"
  },
  "keywords": [
    "prisma",
    "admin",
    "crud",
    "react",
    "ui",
    "components",
    "admin-panel",
    "dashboard"
  ]
}
