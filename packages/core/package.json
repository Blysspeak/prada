{
  "name": "@prada/core",
  "version": "1.0.0",
  "description": "PRADA Core - Schema parser and API generator",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "tsup src/index.js --watch --format esm",
    "build": "tsup src/index.js --format esm --clean",
    "lint": "eslint src --ext .js"
  },
  "dependencies": {
    "@prisma/internals": "^6.0.0",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0"
  },
  "peerDependencies": {
    "@prisma/client": ">=5.0.0"
  }
}
