{
  "name": "@prada/core",
  "version": "1.0.0",
  "description": "PRADA Core - Schema parser and API generator for Prisma",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "tsup src/index.js --watch --format esm",
    "build": "tsup src/index.js --format esm --clean",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@prisma/internals": "^6.0.0",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0"
  },
  "peerDependencies": {
    "@prisma/client": ">=5.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": "blysspeak",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blysspeak/prada.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/blysspeak/prada#readme",
  "bugs": {
    "url": "https://github.com/blysspeak/prada/issues"
  },
  "keywords": [
    "prisma",
    "admin",
    "crud",
    "api-generator",
    "schema-parser",
    "database",
    "postgresql"
  ]
}
