{
  "name": "@blysspeak/prada-server",
  "version": "1.0.0",
  "description": "PRADA Server - Express middleware for Prisma Admin Panel",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "tsup src/index.js --watch --format esm",
    "build": "tsup src/index.js --format esm --clean",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@blysspeak/prada-core": "workspace:*",
    "@blysspeak/prada-ui": "workspace:*",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "jsonwebtoken": "^9.0.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0"
  },
  "peerDependencies": {
    "@prisma/client": ">=5.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": "blysspeak",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blysspeak/prada.git",
    "directory": "packages/server"
  },
  "homepage": "https://github.com/blysspeak/prada#readme",
  "bugs": {
    "url": "https://github.com/blysspeak/prada/issues"
  },
  "keywords": [
    "prisma",
    "admin",
    "crud",
    "express",
    "api",
    "middleware",
    "database",
    "postgresql"
  ]
}
