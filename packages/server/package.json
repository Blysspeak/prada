{
  "name": "@prada/server",
  "version": "1.0.0",
  "description": "PRADA Server - Express API for Prisma Admin",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "tsup src/index.js --watch --format esm",
    "build": "tsup src/index.js --format esm --clean",
    "lint": "eslint src --ext .js"
  },
  "dependencies": {
    "@prada/core": "workspace:*",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.21.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0"
  },
  "peerDependencies": {
    "@prisma/client": ">=5.0.0"
  }
}
