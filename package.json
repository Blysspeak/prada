{
  "name": "@blysspeak/prada",
  "version": "1.0.0",
  "description": "PRADA - PRisma ADmin: Modular admin panel library for PostgreSQL databases",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./ui": {
      "import": "@blysspeak/prada-ui",
      "types": "@blysspeak/prada-ui"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "dev": "pnpm --filter @blysspeak/prada-ui dev",
    "build": "pnpm build:main && pnpm build:ui",
    "build:main": "tsup",
    "build:ui": "pnpm --filter @blysspeak/prada-ui build",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@prisma/internals": "^6.0.0",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "jsonwebtoken": "^9.0.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.10.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0"
  },
  "peerDependencies": {
    "@prisma/client": ">=5.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "prisma",
    "admin",
    "crud",
    "react",
    "express",
    "typescript",
    "admin-panel",
    "database",
    "postgresql",
    "api-generator"
  ],
  "author": "blysspeak",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blysspeak/prada.git"
  },
  "homepage": "https://github.com/blysspeak/prada#readme",
  "bugs": {
    "url": "https://github.com/blysspeak/prada/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
